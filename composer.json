{
  "name": "hustlahusky/spaceonfire2warp",
  "description": "",
  "type": "project",
  "keywords": [],
  "homepage": "https://github.com/hustlahusky/spaceonfire2warp",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://hustlahusky.me",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-pcntl": "*",
    "amphp/file": "^2.0",
    "amphp/parallel": "^1.4",
    "getwarp/clock": "^3.0",
    "getwarp/collection": "^3.0",
    "getwarp/command-bus": "^3.0",
    "getwarp/common": "^3.0",
    "getwarp/container": "^3.0",
    "getwarp/criteria": "^3.0",
    "getwarp/cycle-bridge": "^3.0",
    "getwarp/data-source": "^3.0",
    "getwarp/easy-coding-standard-bridge": "^3.0",
    "getwarp/exception": "^3.0",
    "getwarp/laminas-hydrator-bridge": "^3.0",
    "getwarp/type": "^3.0",
    "getwarp/value-object": "^3.0",
    "nikic/php-parser": "^4.13",
    "roave/better-reflection": "^5.4",
    "symfony/console": "^5.4",
    "yiisoft/arrays": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan": "^1.6",
    "phpunit/phpunit": "^9.5",
    "symplify/easy-coding-standard": "^10.2"
  },
  "replace": {
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "platform-check": false,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "spaceonfire2warp": {
      "directory": "tests/stubs"
    }
  }
}
